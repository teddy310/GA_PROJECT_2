<!DOCTYPE html>
<html>
<head>
  <title>Wholesale</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <!-- Custom Fonts -->
  <link href="http://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic" rel="stylesheet" type="text/css">

</head>


<body>

  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">
          <!-- <img alt="Brand" src=""> -->
        </a>
      </div>
      <%= link_to("Home", root_path, class: "btn btn-primary navbar-btn") %>
      <% if current_user %>
      Sick Email <%= current_user.email %>



      <button type="button" class="btn btn-default navbar-btn"><%= link_to("Update profile", edit_user_path(current_user))%></button>

      <button type="button" class="btn btn-default navbar-btn"><%= link_to 'Logout', logout_path, method: :delete %></button>
      <% else %>
      <%= render 'sessions/login' %>
      <% end %>

      <button type="button" class="btn btn-default navbar-btn"><%= link_to("Create an account", new_user_path)%></button>


    </div>
  </nav>


  <% flash.each do |name, msg| -%>
     <%= content_tag :div, msg, class: name %>
   <% end -%>

  <%= yield %>

</body>
</html>
